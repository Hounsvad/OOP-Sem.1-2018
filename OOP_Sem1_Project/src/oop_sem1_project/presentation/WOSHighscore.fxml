<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefWidth="200.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop_sem1_project.presentation.WOSHighscoreController">
    <children>
        <Label alignment="CENTER" prefHeight="50.0" prefWidth="1000.0" text="High Score">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <ListView fx:id="scoreListView" focusTraversable="false" prefHeight="356.0" prefWidth="200.0" />
        <Button mnemonicParsing="false" onAction="#closeButtonClicked" prefWidth="1000.0" text="Close" />
    </children>
</VBox>
